@model IEnumerable<Tasky.Models.User>


    @foreach (var item in Model)
    {
          <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">@item.Name  </h6>
        </div>
        <div class="card-body">
            @item.Username
        </div>
    </div>
    }

  